#left-panel
  top: 0

#main
  height: 100%

.main-content
  height: 100%
  padding: 14px
  padding-bottom: 25px
  position: relative
  left: 0

.scrolling
  overflow-y: scroll
  height: 100%

.no-select
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
